<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="XmlProformaInvoice">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" name="ProformaInvoiceBases">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="ProformaInvoiceBase">
                <xs:annotation>
                  <xs:documentation>Основание - Основание выставления счета на оплату</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:choice>
                    <xs:element name="OtherBase" type="string1000">
                      <xs:annotation>
                        <xs:documentation>ОснИное - Основание счета на оплату: иные сведения</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ProformaInvoiceBaseDocument" type="DocumentRequisitesType">
                      <xs:annotation>
                        <xs:documentation>ОснРеквДок - Основание счета на оплату: реквизиты документа</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:choice>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="MoneyRecipient" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>ПолучДнСр - Получатель денежных средств</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Payer" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>СвПлат - Сведения о плательщике счета на оплату</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Seller" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>СвПрод - сведения о продавце</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Buyer" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>СвПокуп - сведения о покупателе</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Shipper" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>ГрузОт - грузоотправитель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="Consignee" type="ExtendedOrganizationInfoXmlProformaInvoice29">
          <xs:annotation>
            <xs:documentation>ГрузПолуч - грузополучатель</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" name="CommitmentTypes">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="CommitmentType">
                <xs:annotation>
                  <xs:documentation>ВидОбяз - Вид обязательств</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="CommitmentTypeCode" type="string255">
                    <xs:annotation>
                      <xs:documentation>КодВидОбяз - Код вида обязательств</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="CommitmentTypeName" type="string255">
                    <xs:annotation>
                      <xs:documentation>НаимВидОбязм - Наименование вида обязательств
											</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="ProformaInvoiceAdditionalInfo" type="ProformaInvoiceAdditionalInfo">
          <xs:annotation>
            <xs:documentation>ДопСв - Дополнительные сведения по документу (счету на оплату)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Table" type="ProformaInvoiceTable" />
        <xs:element name="Signers" type="Signers" />
        <xs:element minOccurs="0" name="AdditionalInfos">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="AdditionalInfoId" type="AdditionalInfoId">
                <xs:annotation>
                  <xs:documentation>ИнфПол - Информационное поле</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:all>
      <xs:attribute name="Function" use="required">
        <xs:annotation>
          <xs:documentation>
						Функция - Функция документа
						0 – счет на оплату;
						1 – счет-оферта
					</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:length value="1" />
            <xs:enumeration value="0" />
            <xs:enumeration value="1" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="DocumentDate" type="date" use="required">
        <xs:annotation>
          <xs:documentation>ДатаДок - Дата составления документа (счета на оплату)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DocumentNumber" type="string255" use="required">
        <xs:annotation>
          <xs:documentation>НомерДок - Номер документа (счета на оплату)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Currency" type="okv">
        <xs:annotation>
          <xs:documentation>КодОКВ - валюта (код)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="CurrencyRate">
        <xs:annotation>
          <xs:documentation>КурсВал - Курс валюты</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:totalDigits value="10" />
            <xs:fractionDigits value="4" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute default="false" name="IsConventionalUnit" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>УЕ - Условные единицы</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ConventionalUnitDefinition" type="string255">
        <xs:annotation>
          <xs:documentation>ОпрУЕ - Определение условных единиц</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TotalItems">
        <xs:annotation>
          <xs:documentation>ВсегоНаим - Всего наименований товаров (услуг, работ, иного)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:integer">
            <xs:totalDigits value="6" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ProformaInvoiceTable">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="Item">
        <xs:annotation>
          <xs:documentation>СведТовЦенПок - Сведения о товарах (услугах, работах, ином) и их ценовые показатели</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:all>
            <xs:element minOccurs="0" name="AdditionalInfos">
              <xs:complexType>
                <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="AdditionalInfoId" type="AdditionalInfoId">
                    <xs:annotation>
                      <xs:documentation>ИнфПол - Информационное поле</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="StateSystemItemInfos">
              <xs:complexType>
                <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="StateSystemItemInfo">
                    <xs:annotation>
                      <xs:documentation>СвГосСист - Дополнительные сведения о товаре, подлежащем
												идентификации и учету в государственной информационной системе
											</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element maxOccurs="unbounded" name="StateSystemId" type="string255">
                          <xs:annotation>
                            <xs:documentation>ИдНомУчетЕд - Идентификационный номер учетной
															единицы в государственной информационной системе
														</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                      <xs:attribute name="StateSystemName" type="string255" use="required">
                        <xs:annotation>
                          <xs:documentation>НаимГосСист - Наименование государственной
														информационной системы
													</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="AccountingUnit" type="string255">
                        <xs:annotation>
                          <xs:documentation>УчетЕд - Учетная единица в государственной
														информационной системе
													</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="OtherInfo" type="string1000">
                        <xs:annotation>
                          <xs:documentation>ИнаяИнф - Иная информация о товаре, подлежащем
														учету в государственной информационной системе
													</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:all>
          <xs:attribute name="Product" type="string2000">
            <xs:annotation>
              <xs:documentation>НаимТов - Наименование товара (описание услуг, работ и иного)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Unit" type="okei">
            <xs:annotation>
              <xs:documentation>ОКЕИТов - Код единицы измерения</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="UnitName" type="string255">
            <xs:annotation>
              <xs:documentation>НаимЕдИзм - наименование единицы измерения товара.</xs:documentation>
              <xs:documentation>Пользователь заполняет, если Unit=ОКЕИТов='0000'.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Quantity">
            <xs:annotation>
              <xs:documentation>КолТов - Количество (объем)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="26" />
                <xs:fractionDigits value="11" />
                <xs:minExclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Price">
            <xs:annotation>
              <xs:documentation>ЦенаТов - Цена (тариф) за единицу измерения</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="26" />
                <xs:fractionDigits value="11" />
                <xs:minInclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="PriceWithoutDiscount">
            <xs:annotation>
              <xs:documentation>ЦенаТовБезСки - Цена (тариф) за единицу измерения (без скидки)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="26" />
                <xs:fractionDigits value="11" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="TaxRate" type="TaxRateUtd970">
            <xs:annotation>
              <xs:documentation>НалСт - ставка налога</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="OtherTaxRate" type="string35">
            <xs:annotation>
              <xs:documentation>НалСтИн - Иное значение налоговый ставки</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="DiscountSum">
            <xs:annotation>
              <xs:documentation>СумСки - Сумма скидки</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="DiscountPercent">
            <xs:annotation>
              <xs:documentation>ПроцентСки - Процент скидки</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:integer">
                <xs:totalDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="SubtotalWithVatExcluded" use="required">
            <xs:annotation>
              <xs:documentation>СтТовБезНДС - Стоимость товаров (услуг, работ иного) без налога – всего</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
                <xs:minInclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="SubtotalWithoutVatAndDiscount">
            <xs:annotation>
              <xs:documentation>СтТовБезНДССки - Стоимость товаров (услуг, работ, иного) без налога – всего (без скидки)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Vat">
            <xs:annotation>
              <xs:documentation>СумНал - сумма налога</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
                <xs:minInclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="VatWithoutDiscount">
            <xs:annotation>
              <xs:documentation>СумНалБезСки - Сумма налога, предъявляемая покупателю (без скидки)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
                <xs:minInclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute default="false" name="WithoutVat">
            <xs:annotation>
              <xs:documentation>Без НДС. Указывается, если для товара не требуется НДС</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="true" />
                <xs:enumeration value="false" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="Subtotal" use="required">
            <xs:annotation>
              <xs:documentation>СтТовУчНал - Стоимость товаров (услуг, работ, иного) с налогом – всего</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
                <xs:minInclusive value="0" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="SubtotalWithoutDiscount">
            <xs:annotation>
              <xs:documentation>СтТовУчНалБезСки - Стоимость товаров (услуг, работ, иного) с налогом – всего (без скидки)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:decimal">
                <xs:totalDigits value="19" />
                <xs:fractionDigits value="2" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="ItemMark">
            <xs:annotation>
              <xs:documentation>
								ПрТовРаб - Признак товар-работа-услуга
								1 - имущество;
								2 - работа;
								3 - услуга;
								4 – имущественные права;
								5 - иное
							</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:integer">
                <xs:enumeration value="1" />
                <xs:enumeration value="2" />
                <xs:enumeration value="3" />
                <xs:enumeration value="4" />
                <xs:enumeration value="5" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="AdditionalProperty" type="string4">
            <xs:annotation>
              <xs:documentation>ДопПризн - Дополнительная информация о признаке</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ItemVendorCode" type="string100">
            <xs:annotation>
              <xs:documentation>КодТов - Код товара (в том числе графа 1а счета-фактуры)
							</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="Country">
            <xs:annotation>
              <xs:documentation>
								КодПроисх - Код страны происхождения товара по Общероссийскому
								классификатору стран мира (ОКСМ) или
								980 - Евросоюз,
								981 - ЕАЭС
							</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:length value="3" />
                <xs:pattern value="[0-9]{3}" />
              </xs:restriction>
            </xs:simpleType>
          </xs:attribute>
          <xs:attribute name="ItemCharact" type="string1000">
            <xs:annotation>
              <xs:documentation>ХарактерТов - Характеристика/описание товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ItemArticle" type="string50">
            <xs:annotation>
              <xs:documentation>АртикулТов - Артикул товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ItemKind" type="string10">
            <xs:annotation>
              <xs:documentation>СортТов - Сорт товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ItemSeries" type="string50">
            <xs:annotation>
              <xs:documentation>СерияТов - Серия товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="CatalogCode" type="string27f">
            <xs:annotation>
              <xs:documentation>КодКат - Код каталога</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="ItemTypeCode" type="string10">
            <xs:annotation>
              <xs:documentation>КодВидТов - Код вида товара</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="TotalWithVatExcluded" use="required">
      <xs:annotation>
        <xs:documentation>СтТовБезНДСВсего - Сумма без учета налога</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="19" />
          <xs:fractionDigits value="2" />
          <xs:minInclusive value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Vat">
      <xs:annotation>
        <xs:documentation>СумНалВсего - Сумма налога всего</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="19" />
          <xs:fractionDigits value="2" />
          <xs:minInclusive value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute default="false" name="WithoutVat">
      <xs:annotation>
        <xs:documentation>Без НДС. Указывается, если для суммы налога всего не требуется НДС</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="true" />
          <xs:enumeration value="false" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Total" use="required">
      <xs:annotation>
        <xs:documentation>СтТовУчНалВсего - Сумма всего</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="19" />
          <xs:fractionDigits value="2" />
          <xs:minInclusive value="0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="TotalDiscountSum">
      <xs:annotation>
        <xs:documentation>СумСкиВсего - Сумма скидки по документу</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:decimal">
          <xs:totalDigits value="19" />
          <xs:fractionDigits value="2" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="ProformaInvoiceAdditionalInfo">
    <xs:all>
      <xs:element minOccurs="0" name="PaymentTerms">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="20" name="PaymentTerm" type="string1000">
              <xs:annotation>
                <xs:documentation>УслОпл - Условия оплаты</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="ShipmentTerms">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="20" name="ShipmentTerm" type="string1000">
              <xs:annotation>
                <xs:documentation>УслОказПост - Условия оказания услуги/поставки товара (иного)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="AdditionalInfos">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" name="AdditionalInfoId" type="AdditionalInfoId">
              <xs:annotation>
                <xs:documentation>ИнфПол - Информационное поле</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
    <xs:attribute name="DateRestriction" type="date">
      <xs:annotation>
        <xs:documentation>ОгранОпДат - Ограничение по сроку (дате) оплаты счета</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="DateAndTimeRestriction" type="datetimeutc">
      <xs:annotation>
        <xs:documentation>ОгранОпДатВр - Ограничение по сроку (дате и времени) оплаты счета</xs:documentation>
      </xs:annotation>
    </xs:attribute>
